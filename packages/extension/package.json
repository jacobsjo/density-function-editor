{
	"name": "df-editor-extension",
	"displayName": "df-editor",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:dfeditor.dfeditor"
	],
	"main": "./dist/extension",
	"contributes": {
		"customEditors": [
			{
				"viewType": "dfeditor.dfeditor",
				"displayName": "Density Function Editor",
				"selector": [
					{
						"filenamePattern": "**/worldgen/density_function/**/*.json"
					}
				],
				"priority": "default"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"watch": "webpack --mode none --watch",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "14.x",
		"@types/vscode": "^1.64.0",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"ts-loader": "^9.2.7",
		"ts-node": "^7.0.1",
		"tslib": "^1.9.3",
		"typescript": "^4.5.5",
		"webpack": "^5.69.1",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"buffer": "^6.0.3",
		"comment-json": "4.2.2",
		"path-browserify": "^1.0.1",
		"df-editor-core": "0.1.0"
	}
}